{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "translate",
  "title": "translate",
  "description": "",
  "icon": "translate.png",
  "author": "ss_hh",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "translate",
      "description": "translate for me",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "host",
      "description": "host for request",
      "type": "textfield",
      "required": true,
      "title": "Host",
      "placeholder": "Please input your host"
    },
    {
      "name": "key",
      "description": "A key for request",
      "type": "password",
      "required": true,
      "title": "API KEY",
      "placeholder": "Please input your api key"
    },
    {
      "name": "proxy",
      "description": "https proxy",
      "type": "textfield",
      "required": false,
      "title": "https proxy",
      "placeholder": "Please input your https proxy"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "@raycast/utils": "^1.15.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.4",
    "node-fetch": "^3.3.2",
    "socks-proxy-agent": "^8.0.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "lodash-es": "^4.17.21",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
